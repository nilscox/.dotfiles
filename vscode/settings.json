{
  "editor.tabSize": 2,
  "editor.fontSize": 12,
  "editor.rulers": [110],
  "editor.cursorBlinking": "solid",
  "editor.fontFamily": "JetBrainsMono Nerd Font Propo",
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.smoothScrolling": true,
  "editor.scrollBeyondLastLine": false,
  "editor.lightbulb.enabled": "off",
  "editor.minimap.enabled": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.quickSuggestions": { "strings": "on" },
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "none",
  "editor.suggest.localityBonus": true,
  "editor.formatOnSave": true,
  "editor.foldingStrategy": "indentation",
  "editor.codeActionsOnSave": { "source.fixAll.eslint": "explicit" },
  "emmet.showExpandedAbbreviation": "never",
  "workbench.startupEditor": "none",
  "workbench.editor.empty.hint": "hidden",
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "material-icon-theme",
  "catppuccin.italicComments": true,
  "catppuccin.accentColor": "mauve",
  "catppuccin.boldKeywords": true,
  "material-icon-theme.hidesExplorerArrows": true,
  "window.title": "${dirty}${rootPath}${separator}${activeEditorMedium}",
  "window.titleSeparator": " | ",
  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "hidden",
  "explorer.autoReveal": false,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.simpleDialog.enable": true,
  "files.insertFinalNewline": true,
  "files.associations": { "*.css": "tailwindcss" },
  "scm.defaultViewMode": "tree",
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.commandsToSkipShell": ["-workbench.action.quickOpen"],
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.wordSeparators": " (){}',\"`─‘’",
  "terminal.integrated.fontSize": 16,
  "keyboard.dispatch": "keyCode",
  "typescript.autoClosingTags": true,
  "typescript.preferences.quoteStyle": "double",
  "typescript.preferences.jsxAttributeCompletionStyle": "none",
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.preferences.renameMatchingJsxTags": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[xml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[vue]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "tailwindCSS.classAttributes": ["class", "classes", "className", "classList"],
  "tailwindCSS.colorDecorators": false,
  "auto-rename-tag.activationOnLanguage": [
    "html",
    "xml",
    "javascriptreact",
    "typescriptreact"
  ]
}
