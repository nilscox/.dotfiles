[global_config]
  inactive_color_offset = 0.7
  title_font = Monaco 8
[keybindings]
  zoom_in = <Primary><Alt>Up
  zoom_out = <Primary><Alt>Down
  zoom_normal = <Primary><Alt>Return
  cycle_next = None
  cycle_prev = None
  go_next = None
  go_prev = None
  rotate_cw = None
  rotate_ccw = None
  split_horiz = <Alt>h
  split_vert = <Alt>v
  close_term = None
  toggle_scrollbar = None
  search = None
  close_window = None
  move_tab_right = <Primary><Shift>Page_Down
  move_tab_left = <Primary><Shift>Page_Up
  toggle_zoom = <Alt>Return
  scaled_zoom = None
  next_tab = <Primary>Page_Down
  prev_tab = <Primary>Page_Up
  reset = None
  reset_clear = None
  hide_window = None
  group_all = None
  ungroup_all = None
  group_tab = None
  ungroup_tab = None
  new_window = <Primary><Shift>n
  new_terminator = None
  broadcast_off = None
  broadcast_group = None
  broadcast_all = None
  insert_number = None
  insert_padded = None
  edit_window_title = None
  edit_tab_title = None
  edit_terminal_title = None
  layout_launcher = None
[profiles]
  [[default]]
    icon_bell = False
    background_color = "#20242c"
    cursor_blink = False
    cursor_shape = ibeam
    cursor_color = "#aaaaaa"
    font = Source Code Pro 9
    foreground_color = "#c0c2c5"
    show_titlebar = False
    scrollback_infinite = True
    palette = "#555753:#bf616a:#a3be8c:#ebcb8b:#5e81ac:#b48ead:#88c0d0:#c0c2c5:#4c566a:#bf616a:#a3be8c:#d08770:#77abe7:#caa6ec:#8fbcbb:#96999f"
    use_system_font = False
  [[light]]
    background_color = "#f6f5f4"
    cursor_color = "#aaaaaa"
    foreground_color = "#241f31"
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#cb4b16:#586e75:#657b83:#839496:#6c71c4:#93a1a1:#fdf6e3"
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
